        </fo:inline>
      </xsl:if>
      <fo:block/>
      <!-- residenza -->
       <!-- cittadinanza -->
      <fo:inline>
        <xsl:value-of select="RispostaCertificato/generalita/statusCittadinanza"/>
      </fo:inline>
      <fo:block>
        <fo:leader leader-pattern="space"/>
      </fo:block>
    </fo:block>
  </xsl:template>
</xsl:stylesheet>