        </fo:inline>
        <fo:inline>
          <xsl:value-of select="RispostaCertificato/indirizzo/kilometro"/>
        </fo:inline>
      </xsl:if>
      <xsl:if test="RispostaCertificato/indirizzo/scala != ''">
